                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Aircraft
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasCompany ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :CompanyName
                        ] ;
        rdfs:comment "Represents a flight operated by an airline." ;
        rdfs:label "Flight" .


